# Test

namespace ConsoleApp2
{
	class Program3
	{
		static void Main(string[] args)
		{
			int size = Convert.ToInt32(Console.ReadLine());
			Console.WriteLine("Result:");
			for (int i = 1; i <= size; i++)
			{
				for (int j = 1; j <= size; j++)
				{
					if (j > i)
					{
						Console.Write('*');
					}
					else
					{
						Console.Write(j);
					}
				}
				Console.WriteLine();
			}
		}
	}
}
		
